@model CreateOrderVM
@{
	ViewData["Title"] = "Delete";
	Layout = "~/Views/Shared/_Layout.cshtml";
}


<center>
	<h1>Delete Order</h1>
</center>

<form asp-action="Delete" asp-controller="SavedOrders" enctype="multipart/form-data">
	<div class="form-group">
		<label for="Name">User Id</label>
		<input class="form-control" type="text" asp-for="ApplicationUserId" name="Name" readonly>
	</div>
	<div class="form-group">
		<label for="Name">Car Id</label>
		<input class="form-control" type="text" asp-for="CarsId" name="Name" readonly>
	</div>
	<a asp-controller="SavedOrders" asp-action="Index" class="btn btn-primary">Back To Index</a>
	<input type="submit" value="حذف" class="btn btn-danger" />



</form>
